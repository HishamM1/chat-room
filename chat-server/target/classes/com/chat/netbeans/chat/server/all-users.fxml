<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.chat.netbeans.chat.server.ShowUsersController">
       <children>
      <GridPane gridLinesVisible="true" prefHeight="400.0" prefWidth="600.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="327.0" minWidth="10.0" prefWidth="300.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="330.0" minWidth="10.0" prefWidth="300.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints maxHeight="129.0" minHeight="0.0" prefHeight="41.0" valignment="CENTER" vgrow="SOMETIMES" />
          <RowConstraints maxHeight="374.0" minHeight="10.0" prefHeight="359.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <ListView fx:id="availableUsersListView" prefHeight="200.0" prefWidth="200.0" GridPane.rowIndex="1">
                <placeholder>
                    <Label text="no users yet" />
                </placeholder>
            </ListView>
            <ListView fx:id="busyUsersListView" prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="1">
                <placeholder>
                    <Label text="no users yet" />
                </placeholder>
            </ListView>
            <Label alignment="BASELINE_CENTER" contentDisplay="CENTER" prefHeight="18.0" prefWidth="317.0" text="Avialable Users" textAlignment="CENTER" />
            <Label alignment="CENTER" prefHeight="18.0" prefWidth="303.0" text="Busy Users" GridPane.columnIndex="1" />
         </children>
      </GridPane>
   </children>
</AnchorPane>
